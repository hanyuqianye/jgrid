<?xml version="1.0" encoding="utf-8"?>
<jnlp
    spec="$jnlpspec"
    codebase="$project.Url"
    href="$outputFile">
  <information>
  
    <title>$project.Name</title>
    <vendor>www.guigarage.de / Hendrik Ebbers</vendor>
    <homepage href="www.guigarage.de"/>
    <description>$project.Description</description>
    <offline-allowed/>
  </information>
  <security>
     <all-permissions/>
  </security>
  <resources>
    <j2se version="1.6+"/>
     $dependencies
  </resources>
  <application-desc main-class="$mainClass"/>
</jnlp>